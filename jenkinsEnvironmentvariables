pipeline {
    agent any
    stages { 
        stage('Environment variables') {
            
            steps {
			
            	echo "Updating Configs"
                sh "sed -i 's@<KINESIS_STATUS_DATA_STREAM>@$KINESIS_STATUS_DATA_STREAM@g' app/kinesis_consumer/status_config.py"
                
            }
        }

    }
}
