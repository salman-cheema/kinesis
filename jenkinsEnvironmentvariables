pipeline {
    agent any
     environment {
        DISABLE_AUTH = 'true'
        DB_ENGINE    = 'sqlite'
        KINESIS_STATUS_DATA_STREAM = env.KINESIS_STATUS_DATA_STREAM
    }
    stages { 
        stage('Environment variables') {
            
            steps {
			
            	echo "Updating Configs"
                echo "Database engine is ${KINESIS_STATUS_DATA_STREAM}"
                
            }
        }

    }
}
